

local Library = {}
function Library.Notification(title, text, icon, duration)
	local titleyes = title or "Notification"
	local textyes = text or "library made by Nitro"
	local iconyes = icon or game.Players:GetUserThumbnailAsync(game.Players.LocalPlayer.UserId, "HeadShot", Enum.ThumbnailSize.Size100x100);
	local durationyes = duration or 5
	game.StarterGui:SetCore("SendNotification",  {
		Title = titleyes;
		Text = textyes;
		Icon = iconyes;
		Duration = durationyes;
	})
end



