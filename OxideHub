	reanimationpage.Visible = false
	settingpage.Visible = true
	Slider:TweenPosition(UDim2.new(0.68, 0, 0.188202247, 0),	
		Enum.EasingDirection.Out, 
		Enum.EasingStyle.Sine,
		.5,                       
		true                
	)
end)


Fling.MouseButton1Click:Connect(function()
	_G.FlingEnabled = not _G.FlingEnabled
	Fling.Text = "Fling: "..tostring(_G.FlingEnabled)
end)

nv.MouseButton1Click:Connect(function()
	_G.ToolFling = not _G.ToolFling
	if _G.ToolFling == false then
		nv.Text = "Tool Mode: Holding"
	else 
		nv.Text = "Tool Mode: Fling"
	end
end)

hc.MouseButton1Click:Connect(function()
	_G.HideCharacter = not _G.HideCharacter
	hc.Text = "Hide Character: "..tostring(_G.HideCharacter)
end)


Main:TweenPosition(UDim2.new(0.15, 0, 0.707, 0),	
	Enum.EasingDirection.Out, 
	Enum.EasingStyle.Sine,
	1,                       
	true                
)
local closed = false
task.wait(1.2)
local oldY = Main.Position.Y.Offset
print(oldY)
Close.MouseButton1Click:Connect(function()
	closed = not closed 

	if closed == true then
		oldY = Main.Position.Y.Offset
		print(oldY)
		Main:TweenPosition(UDim2.new(Main.Position.X.Scale, Main.Position.X.Offset, 1.001, 0),	
			Enum.EasingDirection.Out, 
			Enum.EasingStyle.Sine,
			1.25,                       
			true                
		)
		Frame_15:TweenSize(UDim2.new(1.01, 0, 0.0580848791, 0),	
			Enum.EasingDirection.Out, 
			Enum.EasingStyle.Sine,
			1.25,                       
			true                
		)
		Frame_15.UIGradient.Rotation = 0
		Frame_15:TweenPosition(UDim2.new(-0.005, 0, 0.413052112, 0),	
			Enum.EasingDirection.Out, 
			Enum.EasingStyle.Sine,
			1.25,                       
			true                
		)
	else
		print(oldY)
		Main:TweenPosition(UDim2.new(Main.Position.X.Scale, Main.Position.X.Offset, 0.707, oldY),	
			Enum.EasingDirection.In, 
			Enum.EasingStyle.Sine,
			1.25,                       
			true                
		)
		Frame_15:TweenSize(UDim2.new(0.410958916, 0, 0.0580848791, 0),	
			Enum.EasingDirection.Out, 
			Enum.EasingStyle.Sine,
			1.25,                       
			true                
		)
		Frame_15.UIGradient.Rotation = 180
		Frame_15:TweenPosition(UDim2.new(0.293074667, 0, 0.413052112, 0),	
			Enum.EasingDirection.Out, 
			Enum.EasingStyle.Sine,
			1.25,                       
			true                
		)
	end
end)
