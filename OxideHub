rean.MouseButton1Click:Connect(function()
	reanimationpage.Visible = true
	
	settingpage.Visible = false
	Slider:TweenPosition(UDim2.new(0.033, 0, 0.188202247, 0),	
		Enum.EasingDirection.Out, 
		Enum.EasingStyle.Sine,
		.5,                       
		true   
	)
end)

